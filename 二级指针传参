#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
void test(int** ptr)
{
	printf("num=%d\n", **ptr);
}
int main()
{
	int n = 10;
	int* p = &n;
	int** p = &p;
	test(pp);
	test(&p);
	return 0;
}

#include<stdio.h>
void test(int **p)
{
}
int main()
{
	int* ptr;
	int** pp = &ptr;
	test(&ptr);
	test(pp);
	int* arr[10];//
	test(arr);//指针数组也可以
	return 0;
}
